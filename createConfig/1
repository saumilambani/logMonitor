import ConfigParser
import util.timeStamp as ts  

config = ConfigParser.RawConfigParser()

print  ts.timeStamp() + " Setting up configuration"


config.add_section('Simulator')
# Average duration between two log inputs
config.set('Simulator', 'logGenInterval', 0.05)

with open('console.cfg', 'wb') as configfile:
  config.write(configfile)
